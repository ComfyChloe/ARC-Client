{
  "name": "ARC-OSC-Client",
  "version": "0.0.8",
  "author": "ComfyChloe",
  "description": "Standalone OSC Client for ARC-OSC Server",
  "main": "main.js",
  "private": false,
  "dependencies": {
    "axios": "1.6.7",
    "bonjour": "3.5.0",
    "electron": "28.0.0",
    "express": "4.18.2",
    "naive-ui": "2.38.1",
    "osc": "2.4.5",
    "socket.io-client": "4.7.4",
    "vue": "3.4.21"
  },
  "scripts": {
    "start": "electron .",
    "dev": "electron . --dev",
    "build": "electron-builder",
    "pack": "electron-builder --dir"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "5.0.0",
    "electron-builder": "24.9.1",
    "vite": "5.0.0"
  },
  "build": {
    "appId": "com.arcrosc.oscclient",
    "productName": "ARC-OSC Client",
    "directories": {
      "output": "dist"
    },
    "files": [
      "main.js",
      "renderer/**/*",
      "package.json"
    ],
    "win": {
      "target": "nsis",
      "icon": "assets/icon.ico"
    }
  }
}
